! To only apply colors to your terminal, for example, prefix
! the color assignment statement with its name. Example:
!
! URxvt*background:            S_base03

pantheon-terminal*background:              S_base03
pantheon-terminal*foreground:              S_base0
pantheon-terminal*fading:                  40
pantheon-terminal*fadeColor:               S_base03
pantheon-terminal*cursorColor:             S_base1
pantheon-terminal*pointerColorBackground:  S_base01
pantheon-terminal*pointerColorForeground:  S_base1

pantheon-terminal*color0:                  S_base02
pantheon-terminal*color1:                  S_red
pantheon-terminal*color2:                  S_green
pantheon-terminal*color3:                  S_yellow
pantheon-terminal*color4:                  S_blue
pantheon-terminal*color5:                  S_magenta
pantheon-terminal*color6:                  S_cyan
pantheon-terminal*color7:                  S_base2
pantheon-terminal*color9:                  S_orange
pantheon-terminal*color8:                  S_base03
pantheon-terminal*color10:                 S_base01
pantheon-terminal*color11:                 S_base00
pantheon-terminal*color12:                 S_base0
pantheon-terminal*color13:                 S_violet
pantheon-terminal*color14:                 S_base1
pantheon-terminal*color15:                 S_base3
